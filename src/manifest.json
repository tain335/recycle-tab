{
  "manifest_version": 3,
  "name": "Recycle Tab",
  "description": "Recycle Tab",
  "options_page": "options.html",
  "permissions": [
    "offscreen",
    "tabs",
    "storage",
    "unlimitedStorage",
    "activeTab",
    "contextMenus",
    "history",
    "tabGroups",
    "notifications",
    "webRequest",
    "declarativeNetRequest",
    "scripting",
    "webNavigation"
  ],
  "declarative_net_request": {
    "rule_resources": []
  },
  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "service_worker": "background.bundle.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon-32.png"
  },
  "icons": {
    "128": "icon-192.png"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "icon-192.png",
        "icon-32.png",
        "PingFang-SC-Regular.ttf"
      ],
      "matches": []
    }
  ]
}